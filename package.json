		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@changesets/changelog-github": "^0.5.0",
		"@changesets/cli": "^2.27.9",
		"@cloudflare/workers-types": "^4.20241022.0",
		"@types/mock-fs": "^4.13.4",
		"@types/node": "^22.9.0",
		"@types/semver": "^7.5.8",
		"@vercel/ncc": "^0.38.2",
		"mock-fs": "^5.4.1",
		"msw": "^2.6.4",
		"prettier": "^3.3.3",
		"semver": "^7.6.3",
		"typescript": "^5.6.3",
		"vitest": "^2.1.4"
	}
